{
  "manifest_version": 3,
  "name": "Customizable New Tab Dashboard",
  "version": "1.0.0",
  "description": "A powerful, fully customizable dashboard that replaces your default Chrome new tab",
  "permissions": [
    "storage",
    "tabs"
  ],
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  }
}
